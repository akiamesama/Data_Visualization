LOAD CSV WITH HEADERS FROM "file:///Users/Scarlett/Desktop/eBusiness/Practicum/generateData/nodes.csv" AS line CREATE (e:employee { name: line.name, emailAddress: line.emailAddress, numOfChats: line.numOfChats, group: line.group, numOfEmails: line.numOfEmails});
LOAD CSV WITH HEADERS FROM "file:///Users/Scarlett/Desktop/eBusiness/Practicum/generateData/relations.csv" AS line MATCH (from:employee { name:line.from }) MATCH (to:employee { name:line.to }) CREATE (from)-[:MAIL_TO { timestamp: line.timestamp, frequency: line.frequency}]->(to);
